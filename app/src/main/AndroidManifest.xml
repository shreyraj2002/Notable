<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.notable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/notes_launcher_icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/notes_launcher_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Notable">
        <activity
            android:name=".Activity.update_notes"
            android:exported="false" />
        <activity
            android:name=".Activity.Insert_note"
            android:exported="false" />
        <activity
            android:name=".Activity.SplashScreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">

            <!-- android:label="@string/app_name" -->
            <!-- android:theme="@style/Theme.Notable.DarkActionBar"> -->
        </activity>
    </application>

</manifest>